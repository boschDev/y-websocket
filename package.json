{
  "name": "y-websocket",
  "version": "1.4.5",
  "description": "Websockets provider for Yjs",
  "type": "module",
  "main": "./dist/y-websocket.cjs",
  "module": "./src/y-websocket.js",
  "types": "./dist/src/y-websocket.d.ts",
  "sideEffects": false,
  "funding": {
    "type": "GitHub Sponsors ‚ù§",
    "url": "https://github.com/sponsors/dmonad"
  },
  "scripts": {
    "start": "node ./bin/server.js",
    "dist": "rm -rf dist && rollup -c && tsc",
    "lint": "standard && tsc",
    "test": "npm run lint",
    "preversion": "npm run lint && npm run dist && test -e dist/src/y-websocket.d.ts && test -e dist/y-websocket.cjs"
  },
  "bin": {
    "y-websocket-server": "./bin/server.js"
  },
  "files": [
    "dist/*",
    "bin/*",
    "src/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./bin/utils": "./bin/utils.js",
    "./bin/utils.js": "./bin/utils.js",
    "./bin/callback": "./bin/callback.js",
    "./bin/callback.js": "./bin/callback.js",
    ".": {
      "import": "./src/y-websocket.js",
      "require": "./dist/y-websocket.cjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": ["./dist/src/y-websocket.d.ts"],
      "./*": ["./dist/src/*.d.ts"],
      "bin/*": ["./dist/bin/*.d.ts"]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yjs/y-websocket.git"
  },
  "keywords": [
    "Yjs"
  ],
  "author": "Kevin Jahns <kevin.jahns@protonmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yjs/y-websocket/issues"
  },
  "homepage": "https://github.com/yjs/y-websocket#readme",
  "standard": {
    "ignore": [
      "/dist",
      "/node_modules"
    ]
  },
  "dependencies": {
    "lib0": "^0.2.52",
    "lodash.debounce": "^4.0.8",
    "ws": "^8.10.0",
    "y-protocols": "^1.0.5"
  },
  "devDependencies": {
    "@types/node": "^18.11.8",
    "rollup": "^3.2.5",
    "rollup-cli": "^1.0.9",
    "standard": "^17.0.0",
    "typescript": "^4.8.4",
    "yjs": "^13.5.42"
  },
  "peerDependencies": {
    "yjs": "^13.5.6"
  },
  "optionalDependencies": {
    "y-leveldb": "^0.1.1"
  }
}
